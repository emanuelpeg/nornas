<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.assembly.nornas.model.tag.Tag" table="TAGS">
    
    <id column="ID_TAG" name="id">
      <generator class="native"/>
    </id>
    
    <version column="version" name="version" type="java.lang.Long"/>
    
    <property column="NAME" length="255" name="name" not-null="true" type="string"  />
    
  </class>
</hibernate-mapping>
