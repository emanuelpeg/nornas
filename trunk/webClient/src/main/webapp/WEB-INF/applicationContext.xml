<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />

	 <jaxws:client id="user.service"
                  serviceClass="org.assembly.nornas.service.user.UserService"
                  address="http://localhost:8080/web/serviceSca.user" 
                  wsdlLocation="http://localhost:8080/web/serviceSca.user?wsdl" />
                  
     <jaxws:client id="blog.service"
                  serviceClass="org.assembly.nornas.service.blog.BlogService"
                  address="http://localhost:8080/web/serviceSca.blog" 
                  wsdlLocation="http://localhost:8080/web/serviceSca.blog?wsdl" />
                  
     <jaxws:client id="post.service"
                  serviceClass="org.assembly.nornas.service.post.PostService"
                  address="http://localhost:8080/web/serviceSca.post" 
                  wsdlLocation="http://localhost:8080/web/serviceSca.post?wsdl" /> 
	  
</beans>
